<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.supers.basic.mapper.SysLoginMapper">
    <resultMap id="BaseMap" type="com.supers.basic.domain.entity.SysLogin">
        <result column="login_id" property="loginId" jdbcType="DECIMAL"/>
        <result column="login_num" property="loginNum" jdbcType="VARCHAR"/>
        <result column="password" property="password" jdbcType="VARCHAR"/>
        <result column="user_id" property="userId" jdbcType="DECIMAL"/>
    </resultMap>
    <select id="listLogin" parameterType="com.supers.basic.domain.entity.SysLogin" resultMap="BaseMap">
        select login_id,
        login_num,
        password,
        user_id
        from sys_login
        where 1 = 1
    </select>
</mapper>